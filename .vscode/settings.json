{
  "editor.tabSize": 2,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "tailwindCSS.lint.invalidConfigPath": "ignore",
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "cSpell.words": [
    "LONGTAF",
    "METAR",
    "SIGMET"
  ],
}